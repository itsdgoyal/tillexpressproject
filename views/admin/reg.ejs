<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/style.css">
    <title>Document</title>
</head>

<body>
    <%- include('../../partials/admin/adminheader') %>
        <section id="mid">
            <div class="container">
                <div class="row">
                    <%- include('../../partials/admin/adminleft.ejs') %>
                        <div class="col-md-8">
                            <table class="table table-striped mt-2">
                                <tr>
                                    <th>S No</th>
                                    <th>User name</th>
                                    <th>Email</th>
                                    <th>Date</th>
                                    <th colspan="2">Action</th>
                                </tr>
                                <% regrecord.forEach(result=>{%>
                                    <tr>
                                        <td>1</td>
                                        <td>
                                            <%= result.username%>
                                        </td>
                                        <td>
                                            <%= result.email%>
                                        </td>
                                        <td>hhh</td>
                                        <td>
                                            <form action="/admin/regstatusupdate/<%= result.id%>" method="post">
                                                <button type="submit" class="btn btn-success form form-control">
                                                    <%= result.status%>
                                                </button>
                                            </form>
                                        </td>
                                        <td><a href="/admin/contactdelete/">Delete</a> </td>
                                    </tr>
                                    <%})%>
                                    
                            </table>
                        </div>
                </div>
            </div>
        </section>
</body>

</html>